<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
	<Button
		id="idButtonFragmentController2"
		text="{
			formatter: '.formatter.formatValueFragmentController2',
				test: [1, 2, 3],
				test2: ['abc',

				'asd'],
				test3: {
				test: [1, 2, 3],
				test2: ['abc', 'asd']
			},
														test4: [{
						path: 'asd'
			}, {
				path: 123
			}]
		}"
		press="eventHandler11"
	/>
	<Button text="{= ${/SupportModel}}"/>
	<Button text="{Test}"/>
	<Button
		icon="asd"
		text="{
				path: 'test'
		}"
	/>
		<Text
			text="{
				path: 'Test>Confidence',
				type: 'sap.ui.model.type.Integer'
			} %"
			wrapping="false"
		/>
	<Text
		text="{
							path: 'Test>Confidence',
							type: 'sap.ui.model.type.Integer'
						} {
			path: 'Test>Confidence',
					type: 'sap.ui.model.type.Integer'
		}"
		wrapping="false"
	/>
	<HBox
		justifyContent="End"
		visible="{=
			${ZoneModel>ProcessingState} === '1' ||
					${ZoneModel>ProcessingState} === '6' ||
			${ZoneModel>ProcessingState} === '5'
		}"
	/>
	<HBox visible="{=
					${device>/isDesktop} &amp;&amp;
		${
					path: 'SupportModel>asd',
			formatter: '.documentStatusMapping.getPhInvGroupIsPostingAllowed'
		}
	}"/>
	<Button
	visible="{=
								((
								${viewModel>IS_SINGLE_PRODUCT_OFFER}
								) || (
								(
								${viewModel>LEVEL} === 'STORE_FORMAT_LEVEL' ||
								${viewModel>LEVEL} === 'PRODUCT_LEVEL'
								) &amp;&amp;
								!${viewModel>IS_SINGLE_PRODUCT_OFFER}
								))
							}"
	>


	</Button>
	<Button
	visible="{=
								!(${SupportModel>/Layout}).startsWith('Three')
							}"
	>

	</Button>
	<Button
	text="({viewModel>PreviousSimulationPlaceholder/FORECASTED_UNITS})"

							icon="({path: 'viewModel>PreviousSimulationPlaceholder/PROMO_REVENUE',
										formatter: 'window.sir.Util.setFormatNumberCCY2'
								})"
								tooltip="{=
							parseFloat(${viewModel>PROMO_PURCHASE_PRICE_WITHOUT_VAT}) &gt; parseFloat(${viewModel>REGULAR_PURCHASE_PRICE}) ?
							'Promo Purchase Price (' + ${viewModel>PROMO_PURCHASE_PRICE_WITHOUT_VAT} + ') is greater than Regular Purchase Price (' + ${viewModel>REGULAR_PURCHASE_PRICE} + ')' :
							''
						}"

	>

	</Button>
</c:FragmentDefinition>